<Window x:Class="FlowCopy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlowCopy"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="507,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="354" Width="283"/>
        <Button x:Name="button" Content="Read Clipboard" HorizontalAlignment="Left" Margin="663,390,0,0" VerticalAlignment="Top" Click="button_Click"/>
        <ListBox x:Name="listBox_templates" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,606,163" SelectionChanged="listBox_SelectionChanged"/>
        <TextBlock x:Name="textBlock_template" HorizontalAlignment="Left" Margin="199,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="354" Width="283"/>
        <Button x:Name="button1" Content="Start watching clipboard" HorizontalAlignment="Left" Margin="443,390,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox_tags" HorizontalAlignment="Left" Margin="15,301,0,0" VerticalAlignment="Top" Width="184"/>
        <Button x:Name="button_tags" Content="Apply Tags" HorizontalAlignment="Left" Margin="587,390,0,0" VerticalAlignment="Top" Click="button_tags_Click"/>

    </Grid>
</Window>
